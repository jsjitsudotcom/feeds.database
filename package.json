{
  "name": "database",
  "version": "1.1.0",
  "description": "",
  "main": "src/index.js",
  "engines": {
    "node": ">=8.12.0"
  },
  "bin": {
    "db-start": "docker-compose up"
  },
  "scripts": {
    "preinstall": "node scripts/preinstall.js",
    "test": "jest --watch",
    "test:int": "jest --watch int.test.js",
    "test:unit": "jest --watch unit.test.js",
    "test:ci": "CI=true jest --coverage --forceExit",
    "migrate": "sequelize db:migrate",
    "migrate:production": "NODE_ENV=production sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "coveralls": "cat ./coverage/lcov.info | npx coveralls",
    "do:release": "corp-semantic-release",
    "release": "npm run do:release"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.15.1"
  },
  "devDependencies": {
    "commitizen": "^3.0.5",
    "corp-semantic-release": "^6.4.0",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "esdoc": "^1.1.0",
    "esdoc-node": "^1.0.4",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc-undocumented-identifier-plugin": "^1.0.0",
    "eslint": "^5.12.1",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-node": "^8.0.1",
    "ghooks": "^2.0.2",
    "husky": "^0.14.3",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.10.2",
    "sequelize-cli": "^5.4.0",
    "sinon": "^4.1.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {}
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
